<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<VBox styleClass="bordered" alignment="TOP_CENTER" spacing="10.0" xmlns="http://javafx.com/javafx/8"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.aabrasha.view.menus.DirectoryMenuPresenter">
    <children>
        <Button fx:id="btnEmployees" layoutX="10.0" layoutY="10.0" maxWidth="1.7976931348623157E308"
                mnemonicParsing="false" text="Сотрудники" VBox.vgrow="NEVER" onAction="#onBtnEmployeesClicked"/>
        <Button fx:id="btnCompany" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="О компании"
                VBox.vgrow="NEVER" onAction="#onBtnCompanyClicked"/>
        <Button fx:id="btnAuthority" layoutX="10.0" layoutY="35.0" maxWidth="1.7976931348623157E308"
                mnemonicParsing="false" text="Начальство" VBox.vgrow="NEVER"/>
        <Button fx:id="btnDirectory" layoutX="10.0" layoutY="60.0" maxWidth="1.7976931348623157E308"
                mnemonicParsing="false" text="Справочник" VBox.vgrow="NEVER"/>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</VBox>
